              "id": {
                "type": "Type.STRING"
              },
              "name": {
                "type": "Type.STRING"
              },
              "tags": {
                "type": "Type.ARRAY",
                "items": {
                  "type": "Type.STRING"
                }
              },
              "content": {
                "type": "Type.STRING"
              },
              "answer": {
                "type": "Type.ARRAY",
                "items": {
                  "type": "Type.OBJECT",
                  "properties": {
                    "solution": {
                      "type": "Type.STRING"
                    },
                    "is_best_solution": {
                      "type": "Type.BOOLEAN"
                    }
                  },
                  "propertyOrdering": ["solution", "is_best_solution"]
                }
              },
              "hints": {
                "type": "Type.ARRAY",
                "items": {
                  "type": "Type.STRING"
                }
              },
              "difficulty": {
                "type": "Type.STRING"
              },
              "language": {
                "type": "Type.STRING"
              },
              "type": {
                "type": "Type.STRING"
              },
              "created_at": {
                "type": "Type.STRING"
              },
              "updated_at": {
                "type": "Type.STRING"
              },
              "explanation": {
                "type": "Type.STRING"
              },
              "examples": {
                "type": "Type.ARRAY",
                "items": {
                  "type": "Type.OBJECT",
                  "properties": {
                    "input": {
                      "type": "Type.STRING"
                    },
                    "output": {
                      "type": "Type.STRING"
                    }
                  },
                  "propertyOrdering": ["input", "output"]
